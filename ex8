# img8_1

def main(v):
    v1 = int(v[0], 16)
    v2 = int(v[1], 16) << 10
    v3 = int(v[2], 16) << 15
    v4 = int(v[3], 16) << 18
    v5 = int(v[4], 16) << 28

    return str(v1 | v2 | v3 | v4 | v5)



# img8_2

def main(z):
    z = int(z, 16)
    r1 = z & 0b111111
    r2 = (z >> 6) & 0b111
    r3 = (z >> 9) & 0b111
    r4 = (z >> 12) & 0b111111
    return [('C1', int(r1)), ('C2', int(r2)), ('C3', int(r3)), ('C4', int(r4))]
